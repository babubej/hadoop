<coordinator-app name="coord_${feedName}" frequency="${coord:days(1)}"
                 start="${start}" end="${end}" timezone="America/Chicago"
                 xmlns="uri:oozie:coordinator:0.2">
    <controls>
        <timeout>300</timeout>
        <concurrency>10</concurrency>
        <execution>LAST_ONLY</execution>
    </controls>
    <datasets>
        <include>${bundleAppPath}/core_datasets.xml</include>
    </datasets>
    <input-events>
        <data-in name="coordInput" dataset="data_item_loc_combined_fact_core_daily">
            <instance>${coord:current(0)}</instance>
        </data-in>
    </input-events> 
    <input-events>
        <data-in name="coordInputSBT" dataset="data_sbt_extractor_daily">
            <instance>${coord:current(0)}</instance>
        </data-in>
    </input-events> 
    <output-events>
        <data-out name="coordOutput" dataset="data_${feedName}_core_daily">
            <instance>${coord:current(0)}</instance>
        </data-out>
    </output-events> 
    <action>
        <workflow>
            <app-path>${wfAppPath}/wf_rpl_eligibility.xml</app-path>
            <configuration>
                <property>
                    <name>dateStamp</name>
                    <value>${coord:formatTime(coord:nominalTime(),'yyyy-MM-dd')}</value>
                </property>
                <property>
                    <name>nextDayDateStamp</name>
                    <value>${coord:formatTime(coord:dateOffset(coord:nominalTime(), 1, 'DAY'),'yyyy-MM-dd')}</value>
                </property>
                <property>
                    <name>wfInput</name>
                    <value>${coord:dataIn('coordInput')}</value>
                </property>
                <property>
                    <name>wfInputSBT</name>
                    <value>${coord:dataIn('coordInputSBT')}</value>
                </property>
                <property>
                    <name>wfOutput</name>
                    <value>${coord:dataOut('coordOutput')}</value>
                </property>
            </configuration>
        </workflow>
    </action>
</coordinator-app>
